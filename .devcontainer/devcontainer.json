{
  "name": "Cloud Lab",

  "image": "mcr.microsoft.com/devcontainers/python:3.11",

  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "18"
    }
  },

  "postCreateCommand": "pip install flask flask-cors && npm install",

  "forwardPorts": [5173, 7009],

  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "esbenp.prettier-vscode"
      ]
    }
  }
}
